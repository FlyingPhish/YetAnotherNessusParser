          "SSL Certificate Chain Contains Weak RSA Keys",
          "SSL Certificate Signed Using Weak Hashing Algorithm",
          "SSL Certificate with Wrong Hostname",
          "SSL Self-Signed Certificate",
          "SSL.*Certificate.*",
          ".*Certificate.*SSL.*",
          ".*Certificate.*TLS.*",
          ".*Certificate.*Expir.*",
          ".*Certificate.*Invalid.*",
          ".*Certificate.*Trust.*",
          ".*Certificate.*Chain.*",
          ".*Certificate.*Validation.*"
